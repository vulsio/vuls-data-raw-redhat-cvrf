	"documentnotes": {
		"note": [
			{
				"text": "\nRed Hat Ansible Tower 3.6.6-1 - RHEL7 Container\n    ",
				"title": "Topic",
				"type": "Summary",
				"ordinal": "1",
				"lang": "en"
			},
			{
				"text": "\n* Fixed an XSS vulnerability (CVE-2020-25626)\n* Fixed the Red Hat sosreport tool to no longer include the Ansible Tower SECRET_KEY value\n* Fixed the Ansible Tower installer so that it is now compatible with the latest supported Red Hat OpenShift Container Platforms 3.x and 4.x\n    ",
				"title": "Details",
