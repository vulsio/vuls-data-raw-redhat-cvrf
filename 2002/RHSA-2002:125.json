	"documentnotes": {
		"note": [
			{
				"text": "\nUpdated mailman packages are now available for Red Hat Linux Advanced Server. \nThese updates resolve a cross-site scripting vulnerability present in\nversions of Mailman prior to 2.0.11.\n    ",
				"title": "Topic",
				"type": "Summary",
				"ordinal": "1",
				"lang": "en"
			},
			{
				"text": "\nTwo cross-site scripting vulnerabilities have been discovered in versions\nof Mailman prior to version 2.0.11.\n    ",
				"title": "Details",
				"type": "General",
				"ordinal": "2",
