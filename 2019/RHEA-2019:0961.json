	"documentnotes": {
		"note": [
			{
				"text": "\nRed Hat Quay is a secure, private container registry that builds, analyzes and distributes container images. It provides a high level of automation and customization.\n\nThis is the initial release of Red Hat Quay V3.\n    ",
				"title": "Topic",
				"type": "Summary",
				"ordinal": "1",
				"lang": "en"
			},
			{
				"text": "\nRed Hat Quay V3 offers the following new features:\n\n* Red Hat Quay Web UI configuration tool\n* Support for Windows Container Images\n* Multi-Architecture Container Image Support\n* Built on Red Hat Enterprise Linux\n* Red Hat Quay images now in redhat repo on Quay.io\n* Changes to support running containers in unprivileged mode\n    ",
				"title": "Details",
				"type": "General",
				"ordinal": "2",
				"lang": "en"
			},
			{
				"text": "Please see https://www.redhat.com/footer/terms-of-use.html",
				"title": "Terms of Use",
				"type": "Legal Disclaimer",
				"ordinal": "3",
				"lang": "en"
			}
		]
	},
