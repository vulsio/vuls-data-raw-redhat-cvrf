	"documentnotes": {
		"note": [
			{
				"text": "\nThe updated Advanced Virtualization module is now available for Red Hat Enterprise Linux 8.1 Advanced Virtualization.\n    ",
				"title": "Topic",
				"type": "Summary",
				"ordinal": "1",
				"lang": "en"
			},
			{
				"text": "\nThe Advanced Virtualization module provides the user-space component for running virtual machines that use KVM in environments managed by Red Hat products.\n    ",
				"title": "Details",
				"type": "General",
				"ordinal": "2",
				"lang": "en"
			},
