	"documentnotes": {
		"note": [
			{
				"text": "\nA new Zope-Hotfix package is availble which fixes issues with computation\nof local roles.\n    ",
				"title": "Topic",
				"type": "Summary",
				"ordinal": "1",
				"lang": "en"
			},
			{
				"text": "\nAn issue with the computation of local roles exists where the computation\nwas not climbing the proper hierarchy of folders, and sometimes granting\nlocal roles inappropriately.\n\nYou must update Zope to version 2.2.4 prior to applying this update.\n    ",
				"title": "Details",
				"type": "General",
				"ordinal": "2",
				"lang": "en"
			},
